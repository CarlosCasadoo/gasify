import org.junit.jupiter.api.Assertions;
import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;

public class BillingTest {

    private Billing billing;

    @BeforeEach
    void setUp() {
        billing = new Billing();
    }

    @Test
    void testSetAndGetIdReport() {
        int expectedId = 123;
        billing.setIdReport(expectedId);
        int actualId = billing.getIdReport();
        Assertions.assertEquals(expectedId, actualId);
    }

    @Test
    void testSetAndGetIdCustomer() {
        String expectedId = "ABC123";
        billing.setIdCustomer(expectedId);
        String actualId = billing.getIdCustomer();
        Assertions.assertEquals(expectedId, actualId);
    }

    @Test
    void testSetAndGetIdGasMeter() {
        String expectedId = "Meter123";
        billing.setIdGasMeter(expectedId);
        String actualId = billing.getIdGasMeter();
        Assertions.assertEquals(expectedId, actualId);
    }

    @Test
    void testDefaultConstructor() {
        int expectedId = 0;
        String expectedCustomer = null;

        Assertions.assertEquals(expectedId, billing.getIdReport());
        Assertions.assertEquals(expectedCustomer, billing.getIdCustomer());
        
    }
}
